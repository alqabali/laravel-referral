{
    "name": "alqabali/laravel-referral",
    "description": "A Referral System With Laravel",
    "keywords": ["referral", "referral-system", "affiliate-tracking"],
    "license": "MIT",
    "authors": [{
        "name": "alqabali",
        "email": "zhengchaopu@gmail.com"
    }],
    "require": {
        "php": ">=7.0"
    },
    "require-dev": {
        "laravel/laravel": "^8.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "~4.0|~9.0"
    },
    "autoload": {
        "psr-4": {
            "alqabali\\Referral\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "alqabali\\Referral\\ReferralServiceProvider"
            ]
        }
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    }
}
